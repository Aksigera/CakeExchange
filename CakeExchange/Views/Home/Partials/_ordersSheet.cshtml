@model IEnumerable<Order>

<table>
    <tr>
        <th>ID</th>
        <th>Цена</th>
        <th>Кол-во</th>
        <th>Коммент.</th>
        <th>Date</th>
    </tr>

    @foreach (Order order in Model)
    {
        <tr>
            <td>@order.Id</td>
            <td>@order.Price</td>
            <td>@order.Number</td>
            <td>@order.Comment</td>
            <td>@order.Date</td>
        </tr>
    }

</table >