@model IEnumerable<Order>

<table>
    <tr>
        <th>Цена</th>
        <th>Кол-во</th>
        <th>Коммент.</th>
    </tr>

    @foreach (Order order in Model)
    {
        <tr>
            <td>@order.Price.ToString("G29")</td>
            <td>@order.Number</td>
            <td>@order.Comment</td>
        </tr>
    }

</table >